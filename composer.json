{
    "name": "dcodegroup/activity-log",
    "description": "Simple package which dcode uses to manage activity logs",
    "keywords": [
        "laravel",
        "activity logs",
        "communication logs",
        "data"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Thanh Nguyen",
            "email": "thanh@dcodegroup.com",
            "role": "Developer"
        },
        {
            "name": "Dcode Group",
            "email": "forge@dcodegroup.com",
            "homepage": "https://dcodegroup.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "laravel/framework": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "nesbot/carbon": "^2.72",
        "spatie/laravel-query-builder": "^4.0.4|^5.7"
    },
    "autoload": {
        "psr-4": {
            "Dcodegroup\\ActivityLog\\": "src"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dcodegroup\\ActivityLog\\ActivityLogServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/pint": "*"
    }
}
